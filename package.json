{
  "name": "jinaga-react",
  "version": "3.0.0",
  "description": "React binding helpers for Jinaga",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "dev": "nodemon -e ts -w ./src -x npm run test:watch",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "react",
    "viewmodel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/michaellperry/jinaga-react"
  },
  "author": "Michael L Perry",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "jest": "^27.5.1",
    "jinaga": "^3.0.0",
    "nodemon": "^2.0.2",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "peerDependencies": {
    "jinaga": "^2.0 || ^3.0",
    "react": "^16.8 || ^17.0"
  }
}
